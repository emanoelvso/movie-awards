{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@producerModule/(.*)$": "<rootDir>/src/module/producer/$1",
    "^@testInfra/(.*)$": "<rootDir>/test/$1"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testMatch": ["<rootDir>/src/**/__test__/e2e/**/*.spec.ts"]
}
